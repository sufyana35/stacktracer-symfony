{
    "name": "stacktracer/stacktracer-symfony",
    "description": "Lightweight error tracking and tracing SDK for Symfony applications. Capture exceptions, request traces, and performance data.",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "symfony",
        "tracing",
        "error-tracking",
        "monitoring",
        "apm",
        "observability",
        "bundle",
        "exceptions",
        "debugging"
    ],
    "homepage": "https://github.com/stacktracer/stacktracer-symfony",
    "authors": [
        {
            "name": "Stacktracer",
            "email": "hello@stacktracer.io"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/framework-bundle": "^6.4 || ^7.0 || ^8.0",
        "symfony/http-kernel": "^6.4 || ^7.0 || ^8.0",
        "symfony/event-dispatcher": "^6.4 || ^7.0 || ^8.0",
        "symfony/dependency-injection": "^6.4 || ^7.0 || ^8.0",
        "symfony/config": "^6.4 || ^7.0 || ^8.0",
        "psr/log": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Stacktracer\\SymfonyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stacktracer\\SymfonyBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "^6.4 || ^7.0 || ^8.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
